<h3>Add interesting</h3>
<%= form_for :interesting, url: interestings_path, method: :post do |f|%>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :user_name, class: "form-control", placeholder: "John Doe" %>
  </div>
  <div class="form-group">
    <%= f.label :interesting_title %>
    <%= f.text_field :title, class: "form-control", placeholder: "Programming your own drone" %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: "form-control", placeholder: "Buy a drone..." %>
  </div>
  <%= f.submit "Create", class: "btn btn-primary" %>
<% end %>