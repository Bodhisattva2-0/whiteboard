<h3>Create new interesting</h3>
<%= form_for :interesting, url: interestings_path, method: :post do |f|%>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :user_name %>
  <%= f.text_field :user_name %>

  <%= f.submit "Create"%>
<% end %>