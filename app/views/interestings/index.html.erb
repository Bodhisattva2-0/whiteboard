<div>
  <a href="/interestings/new" class="btn btn-danger">Add</a>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Interesting title</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <% @interestings.each do |interesting| %>
      <tr>
        <td><i class="fa fa-user"></i><%= interesting.user_name %></td>
        <td><%= interesting.title %></td>
        <td><%= interesting.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
